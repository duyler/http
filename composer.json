{
    "name": "duyler/http",
    "type": "library",
    "description": "Duyler http handler",
    "license": "MIT",
    "keywords": [],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://duyler.org/docs/http/"
    },
    "require": {
        "php": "^8.3",
        "duyler/config": "dev-main",
        "duyler/router": "dev-main",
        "duyler/action-bus": "dev-main",
        "duyler/framework": "dev-main",
        "httpsoft/http-server-request": "^1.1",
        "httpsoft/http-response": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.30"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Duyler\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\Http\\Test\\": ["tests/"]
        }
    }
